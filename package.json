{
  "name": "andalu_woo_courses",
  "version": "0.1.0",
  "main": "Gruntfile.js",
  "author": "TheWebist",
  "devDependencies": {
    "grunt": "^1.2.1",
    "grunt-wp-i18n": "^1.0.3",
    "grunt-wp-readme-to-markdown": "~1.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.29.0"
  },
  "scripts": {
    "compile": "sass lib/scss/class-calendar.scss lib/css/class-calendar.css",
    "development:browsersync": "browser-sync start --proxy 'https://netmind.local' --files 'lib/css/*.css' 'lib/js/src/*.js' 'lib/fns/*.php' 'lib/templates/*.hbs'",
    "development:sasswatch": "sass --watch lib/scss:lib/css",
    "production:sass": "sass --style=compressed --no-source-map lib/scss/class-calendar.scss lib/css/class-calendar.css",
    "dev": "npm-run-all -p development:*",
    "build": "npm-run-all -p production:*"
  }
}
